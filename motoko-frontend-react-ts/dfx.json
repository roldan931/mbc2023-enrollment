{
  "canisters": {
    "counter": {
      "main": "canisters/counter/main.mo",
      "type": "motoko"
    },
    "student_wall": {
      "main": "canisters/student_wall.mo",
      "type": "motoko"
    },
    "assets": {
      "dependencies": [
        "counter",
        "student_wall"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": [
        "dist/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
